{
  "usesTypeScript": true,
  "usesCssModule": true,
  "cssPreprocessor": "scss",
  "testLibrary": "Testing Library",
  "component": {
    "default": {
      "path": "src/components",
      "withLazy": false,
      "withStory": true,
      "withStyle": true,
      "withTest": true,
      "withIndex": true,
      "withReadme": true,
      "customTemplates": {
        "component": "./templates/component/TemplateName.tsx",
        "story":  "./templates/component/TemplateName.stories.tsx",
        "style": "./templates/component/TemplateName.style.scss",
        "test":  "./templates/component/TemplateName.spec.ts",
        "index": "./templates/component/index.ts",
        "readme": "./templates/component/README.md"
      }
    },
    "view": {
      "path": "src/views",
      "withLazy": false,
      "withStory": false,
      "withStyle": true,
      "withTest": true,
      "withIndex": true,
      "withReadme": true,
      "customTemplates": {
        "component": "./templates/view/TemplateName.tsx",
        "style": "./templates/view/TemplateName.style.scss",
        "test":  "./templates/view/TemplateName.spec.ts",
        "index": "./templates/view/index.ts",
        "readme": "./templates/view/README.md"
      }
    },
    "layout": {
      "path": "src/layouts",
      "withLazy": false,
      "withStory": false,
      "withStyle": true,
      "withTest": true,
      "withIndex": true,
      "withReadme": true,
      "customTemplates": {
        "component": "./templates/layout/TemplateName.tsx",
        "style": "./templates/layout/TemplateName.style.scss",
        "test":  "./templates/layout/TemplateName.spec.ts",
        "index": "./templates/layout/index.ts",
        "readme": "./templates/layout/README.md"
      }
    }
  }
}
